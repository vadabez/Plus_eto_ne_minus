{
    "manifest_version": 2,
    "name": "Plus_eto_ne_minus",
    "version": "1.5",
    "description": "Добавляет +1 в Jira",
    "icons": {
        "128": "plus.png"
    },
    "background": {
        "scripts": ["bg.js"],
        "persistent": false
      },
    "content_scripts": [
        {
            "matches": [ "https://jira.skyeng.tech/*","*://skyeng.autofaq.ai/*", "https://crm2.skyeng.ru/*"],
            "js": [ "app.js" ],
            "css": ["mystyles.css"]
        }
    ],
    "permissions": [
        "*://jira.skyeng.tech/*","*://skyeng.autofaq.ai/*", "https://crm2.skyeng.ru/*"
    ]
 }