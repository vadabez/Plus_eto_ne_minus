{
    "manifest_version": 2,
    "name": "Plus_eto_ne_minus",
    "version": "1.0",
    "description": "Добавляет +1 в Jira",
    "icons": {
        "128": "plus.png"
    },
    "background": {
        "scripts": ["bg.js"],
        "persistent": false
      },
    "content_scripts": [
        {
            "matches": [ "https://jira.skyeng.tech/*" ],
            "js": [ "app.js" ]
        }

    ],
    "permissions": [
        "*://*.jira.skyeng.tech/*","*://skyeng.autofaq.ai/*"
    ]
 }